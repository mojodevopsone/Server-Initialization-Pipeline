pipeline {
    agent any
    stages {
        stage ('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/mojodevopsone/ansible-inventory-test.git'
            }
        }
        stage('git') {
            steps {
                git credentialsId: 'jenkins-sshkey', url: 'git@github.com:mojodevopsone/ansible-inventory-test.git'
            }
        }
        stage('ansiblePlaybook') {
            steps {
                ansiblePlaybook inventory: 'inventory.ini', playbook: 'playbook.yml', sudoUser: null, vaultTmpPath: ''

            }
        }
    }
}

